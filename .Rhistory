get_fibonacci <- function(n) {
if (n <= 0) {
stop("Input must be a positive integer.")
}
if (n == 1) {
return(0)
}
else if (n == 2) {
return(1)
}
else {
return(get_fibonacci(n - 1) + get_fibonacci(n - 2))
}
}
get_fibonacci(19)
